<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.consultadobem" version="2.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>AwesomeApp</name>
    <description>
        Demo app for vue-cordova NPM package
    </description>
    <author email="suporte@gmail.com">
        Simon Tarchichi
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-device" spec="~1.1.3" />
    <plugin name="cordova-plugin-geolocation" spec="~2.4.0" />
    <engine name="ios" spec="^4.5.4" />
    <engine name="android" spec="^7.1.1" />
    <engine name="browser" spec="^5.0.3" />
</widget>
